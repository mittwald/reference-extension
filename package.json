{
  "name": "@mittwald/reference-extension",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "init:encryption": "node scripts/generate-encryption-secrets.js",
    "dev": "dotenv -- vite dev --host",
    "dev:expose": "dotenv -- bash -c 'zrok share reserved $ZROK_RESERVED_TOKEN'",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "db:push": "drizzle-kit push",
    "db:generate-migrations": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio --port 8081",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "docker:prod": "docker compose up",
    "docker:prod:build": "docker compose up --build",
    "docker:prod:down": "docker compose down"
  },
  "dependencies": {
    "@mittwald/api-client": "^4.267.0",
    "@mittwald/api-client-commons": "^4.267.0",
    "@mittwald/ext-bridge": "0.2.0-alpha.557",
    "@mittwald/flow-remote-react-components": "0.2.0-alpha.557",
    "@mittwald/mstudio-ext-react-components": "0.2.0-alpha.557",
    "@mittwald/react-ghostmaker": "^1.4.2",
    "@mittwald/react-use-promise": "^4.2.2",
    "@tanstack/nitro-v2-vite-plugin": "^1.139.0",
    "@tanstack/react-devtools": "^0.7.11",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-router": "^1.139.7",
    "@tanstack/react-router-devtools": "^1.139.7",
    "@tanstack/react-router-ssr-query": "^1.139.7",
    "@tanstack/react-start": "^1.139.8",
    "@tanstack/router-plugin": "^1.139.7",
    "@weissaufschwarz/mitthooks": "^0.3.0",
    "@weissaufschwarz/mitthooks-drizzle": "^0.1.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "envalid": "^8.1.1",
    "nitro": "3.0.1-alpha.1",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.66.1",
    "uuid": "^13.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^22.19.1",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.7",
    "jsdom": "^27.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}
